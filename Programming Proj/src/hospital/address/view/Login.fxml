<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hospital.address.view.LoginController">
   <children>
      <GridPane layoutY="1.0" AnchorPane.bottomAnchor="1.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="1.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="374.0" minWidth="10.0" prefWidth="158.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="374.0" minWidth="10.0" prefWidth="116.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="254.0" minWidth="10.0" prefWidth="153.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="195.0" minWidth="10.0" prefWidth="175.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="37.0" minHeight="10.0" prefHeight="33.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="58.0" minHeight="10.0" prefHeight="51.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField fx:id="username" onAction="#initialize" onKeyPressed="#initialize" promptText="username" GridPane.columnIndex="2" GridPane.rowIndex="3" />
            <PasswordField fx:id="password" onKeyPressed="#initialize" prefHeight="25.0" prefWidth="227.0" promptText="password" GridPane.columnIndex="2" GridPane.rowIndex="4" />
            <Button fx:id="loginButton" mnemonicParsing="false" onAction="#initialize" prefHeight="36.0" prefWidth="69.0" text="Login" textAlignment="CENTER" GridPane.columnIndex="2" GridPane.rowIndex="6">
               <GridPane.margin>
                  <Insets left="50.0" />
               </GridPane.margin></Button>
            <Label prefHeight="41.0" prefWidth="373.0" text="Username" textAlignment="CENTER" textFill="#1b1a1a" GridPane.columnIndex="1" GridPane.rowIndex="3">
               <font>
                  <Font name="Arial" size="19.0" />
               </font></Label>
            <Label prefHeight="41.0" prefWidth="370.0" text="WELCOME" textAlignment="CENTER" textFill="#1b1a1a" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="1">
               <font>
                  <Font name="Arial" size="35.0" />
               </font>
            </Label>
            <Label prefHeight="41.0" prefWidth="373.0" text="Password" textAlignment="CENTER" textFill="#0d0d0d" GridPane.columnIndex="1" GridPane.rowIndex="4">
               <font>
                  <Font name="Arial" size="19.0" />
               </font>
            </Label>
            <Label fx:id="wrong" text="Label" textFill="RED" GridPane.columnIndex="2" GridPane.rowIndex="5" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Please login : " GridPane.rowIndex="3">
               <font>
                  <Font name="Arial" size="19.0" />
               </font>
            </Text>
            <Label fx:id="RequireUser" textFill="#ff0808" GridPane.columnIndex="3" GridPane.rowIndex="3" />
            <Label fx:id="RequiredPass" textFill="#f20303" GridPane.columnIndex="3" GridPane.rowIndex="4" />
         </children>
      </GridPane>
   </children>
   <effect>
      <Glow />
   </effect>
</AnchorPane>
